{
	"name": "RiskData",
	"version": "1.1.0",
	"type": "parserhook",
	"author": "[https://www.mediawiki.org/wiki/User:RV1971 RV1971]",
	"descriptionmsg": "riskdata-desc",
	"url": "https://www.mediawiki.org/wiki/Extension:RiskData",
	"license-name": "GPL-3.0-or-later",
	"requires": {
		"MediaWiki": ">= 1.39"
	},
	"AutoloadClasses": {
		"RiskData": "includes/RiskData.php",
		"RiskDataDatabase": "includes/RiskDataDatabase.php",
		"RiskDataException": "includes/RiskDataException.php",
		"RiskDataHooks": "includes/RiskDataHooks.php",
		"RiskDataParser": "includes/RiskDataParser.php",
		"RiskDataParserWithRecords": "includes/RiskDataParser.php",
		"RiskDataSqlTransformer": "includes/RiskDataSqlTransformer.php",
		"Scribunto_LuaRiskDataLibrary": "includes/Scribunto_LuaRiskDataLibrary.php",
		"SpecialRiskData": "includes/SpecialRiskData.php",
		"RiskDataPager": "includes/SpecialRiskData.php",
		"SpecialRiskDataData": "includes/SpecialRiskDataData.php",
		"RiskDataDataPager": "includes/SpecialRiskDataData.php",
		"SpecialRiskDataPages": "includes/SpecialRiskDataPages.php",
		"RiskDataPagesPager": "includes/SpecialRiskDataPages.php",
		"SpecialRiskDataTables": "includes/SpecialRiskDataTables.php",
		"RiskDataTablesPager": "includes/SpecialRiskDataTables.php"
	},
	"ExtensionFunctions": [ "RiskData::init" ],
	"Hooks": {
		"ArticleDelete": "main",
		"LoadExtensionSchemaUpdates": "main",
		"ParserFirstCallInit": "main",
		"RevisionFromEditComplete": "main"
	},
	"HookHandlers": {
		"main": {
			"class": "RiskDataHooks"
		}
	},
	"MessagesDirs": {
		"RiskData": [
			"i18n"
		]
	},
	"ExtensionMessagesFiles": {
		"RiskDataAlias": "includes/RiskData.alias.php",
		"RiskDataMagic": "includes/RiskData.i18n.magic.php"
	},
	"AvailableRights": [ "riskdata-specialpages" ],
	"GroupPermissions": {
		"user": { "riskdata-specialpages": true }
	},
	"SpecialPages": {
		"RiskDataData": "SpecialRiskDataData",
		"RiskDataPages": "SpecialRiskDataPages",
		"RiskDataTables": "SpecialRiskDataTables"
	},
	"config": {
		"RiskDataWriteDest": {
			"description": "Name of the database table where data is written to.",
			"value": "riskdata_data"
		},
		"RiskDataReadSrc": {
			"description": "Name of the database table where data is read from.",
			"value": "riskdata_data"
		},
		"RiskDataMetaWriteDest": {
			"description": "Name of the database table where meta data is written to.",
			"value": "riskdata_meta"
		},
		"RiskDataMetaReadSrc": {
			"description": "Name of the database table where meta data is read from.",
			"value": "riskdata_meta"
		},
		"RiskDataArgs": {
			"description": "Default arguments for riskdata tags.",
			"value": {
				"fs": "|",
				"rs": "/[\n\r]+/"
			}
		},
		"RiskDataSqlWhiteList": {
			"description": "Array of identifiers that may be used in WHERE and ORDER BY clauses, in addition to column names.",
			"value": [
				"ASC", "DESC", "NULLS", "FIRST", "LAST",
				"AND", "NOT", "OR",
				"BETWEEN", "IN", "IS", "LIKE", "NULL",
				"CASE", "WHEN", "THEN", "ELSE", "END", "COALESCE", "NULLIF",
				"CAST", "AS",
				"EXTRACT", "FROM",
				"ABS", "MOD", "LN", "EXP", "POWER", "SQRT", "FLOOR", "CEIL",
				"SUBSTR", "SUBSTRING", "UPPER", "LOWER", "TRANSLATE", "TRIM"
			]
		},
		"SpecialRiskDataPageParSep": {
			"description": "Separator for parameters passed to special pages.",
			"value": "//"
		},
		"SpecialRiskDataDataClasses": {
			"description": "Array of css classes for the table used in Special:RiskDataData.",
			"value": [ "wikitable", "sortable" ]
		}
	},
	"manifest_version": 2
}
